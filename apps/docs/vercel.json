{
  "version": 2,
  "name": "physical-ai-textbook-docs",
  "buildCommand": "npm run build",
  "devCommand": "npm run start",
  "installCommand": "npm install",
  "framework": "docusaurus",
  "outputDirectory": "build",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/api/chatbot/:path*",
      "destination": "https://rameez12-physical-ai-textbook.hf.space/api/chatbot/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
